<UserControl x:Class="Client.MVVM.UserControls.IngameControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Client.MVVM.UserControls"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <TextBlock Text="{Binding TestString}" Foreground="White"></TextBlock>
        <StackPanel Margin="0,50,0,0">
            <Button x:Name="EnableLiveEvent" Content="Enable LiveEventAPI" Width="250" Height="50" Click="EnableLiveEvent_Click"/>
            <Button x:Name="DisableLiveEvent" Content="Disable LiveEventAPI" Width="250" Height="50" Margin="0,10,0,0" Click="DisableLiveEvent_Click"/>

        </StackPanel>
        
    </Grid>
</UserControl>
